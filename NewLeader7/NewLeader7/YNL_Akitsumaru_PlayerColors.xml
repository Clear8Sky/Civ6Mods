<?xml version="1.0" encoding="utf-8"?>
<Database>
	<!-- 文明の色 -->
	<!-- ここで指定したものがユニットのアイコンの色になる。 -->
	<!-- PrimaryColorが透過部分、SecondaryColorが白い部分に塗られることになるので、文明のアイコンをそのように作っておく必要がある。 -->
	<PlayerColors>
		<Row>
			<Type>LEADER_YNL_AKITSUMARU</Type>
			<Usage>Unique</Usage>
			<PrimaryColor>COLOR_PLAYER_YNL_AKITSUMARU_PRIMARY</PrimaryColor>
			<SecondaryColor>COLOR_PLAYER_YNL_AKITSUMARU_SECONDARY</SecondaryColor>
			<Alt1PrimaryColor>COLOR_STANDARD_ORANGE_DK</Alt1PrimaryColor>
			<Alt1SecondaryColor>COLOR_STANDARD_GREEN_LT</Alt1SecondaryColor>
			<Alt2PrimaryColor>COLOR_STANDARD_BLUE_MD</Alt2PrimaryColor>
			<Alt2SecondaryColor>COLOR_STANDARD_WHITE_LT</Alt2SecondaryColor>
			<Alt3PrimaryColor>COLOR_STANDARD_YELLOW_MD</Alt3PrimaryColor>
			<Alt3SecondaryColor>COLOR_STANDARD_WHITE_DK</Alt3SecondaryColor>
		</Row>
	</PlayerColors>
</Database>
