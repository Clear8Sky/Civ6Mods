<?xml version="1.0" encoding="utf-8"?>
<Database>
	<!-- 文明の色 -->
	<!-- ここで指定したものがユニットのアイコンの色になる。 -->
	<!-- PrimaryColorが透過部分、SecondaryColorが白い部分に塗られることになるので、文明のアイコンをそのように作っておく必要がある。 -->
	<PlayerColors>
		<Row>
			<Type>LEADER_HARBOUR_PRINCESS</Type>
			<Usage>Unique</Usage>
			<PrimaryColor>COLOR_PLAYER_YNL_HARBOUR_PRINCESS_PRIMARY</PrimaryColor>
			<SecondaryColor>COLOR_PLAYER_YNL_HARBOUR_PRINCESS_SECONDARY</SecondaryColor>
			<Alt1PrimaryColor>COLOR_PLAYER_YNL_HARBOUR_PRINCESS_PRIMARY</Alt1PrimaryColor>
			<Alt1SecondaryColor>COLOR_STANDARD_RED_MD</Alt1SecondaryColor>
			<Alt2PrimaryColor>COLOR_PLAYER_YNL_HARBOUR_PRINCESS_PRIMARY</Alt2PrimaryColor>
			<Alt2SecondaryColor>COLOR_STANDARD_YELLOW_MD</Alt2SecondaryColor>
			<Alt3PrimaryColor>COLOR_PLAYER_YNL_HARBOUR_PRINCESS_PRIMARY</Alt3PrimaryColor>
			<Alt3SecondaryColor>COLOR_STANDARD_AQUA_MD</Alt3SecondaryColor>
		</Row>
	</PlayerColors>
</Database>
