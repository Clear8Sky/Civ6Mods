		<!-- Leader Dialog -->
		<Replace Tag="LOC_DIPLO_ACCEPT_DECLARE_FRIEND_FROM_HUMAN_LEADER_YNL_NAGATO_ANY" Language="ja_JP">
			<Text>これからは仲良くやっていこう。</Text>
		</Replace>
		<Replace Tag="LOC_DIPLO_REJECT_DECLARE_FRIEND_FROM_HUMAN_LEADER_YNL_NAGATO_ANY" Language="ja_JP">
