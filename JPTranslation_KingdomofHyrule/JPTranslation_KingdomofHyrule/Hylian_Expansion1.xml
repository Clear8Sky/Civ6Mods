<?xml version="1.0" encoding="utf-8"?>
<GameData>
	<!-- RaFでは野戦救急車を補給部隊の置換にする -->
	<UnitUpgrades>
		<!-- バニラで有効になっている衛生兵から野戦救急車のアップグレードの記述を消す -->
		<Delete Unit="UNIT_MEDIC" />
		<!-- Delete文はキーが一致するレコードすべてが消されるため、バニラの記述(補給部隊へのアップグレード)を入れなおす -->
		<Row Unit="UNIT_MEDIC" UpgradeUnit="UNIT_SUPPLY_CONVOY"/>
	</UnitUpgrades>
	<UnitReplaces>
		<Row CivUniqueUnitType="UNIT_HYLIAN_AMBULANCE" ReplacesUnitType="UNIT_SUPPLY_CONVOY"/>
	</UnitReplaces>

	<!-- 歴史年表用の画像 -->
	<MomentIllustrations>
		<!-- 時の神殿 -->
		<Row MomentIllustrationType="MOMENT_ILLUSTRATION_UNIQUE_BUILDING"
			 MomentDataType="MOMENT_DATA_BUILDING"
			 GameDataType="BUILDING_TEMPLE_OF_TIME"
			 Texture="YNL_MOMENT_TEMPLE_OF_TIME.dds"/>
		<!-- 時の勇者 -->
		<Row MomentIllustrationType="MOMENT_ILLUSTRATION_UNIQUE_UNIT"
			 MomentDataType="MOMENT_DATA_UNIT"
			 GameDataType="UNIT_HERO_OF_TIME"
			 Texture="YNL_MOMENT_HERO_OF_TIME.dds"/>
		<!-- 妖精の泉 -->
		<Row MomentIllustrationType="MOMENT_ILLUSTRATION_UNIQUE_IMPROVEMENT"
			 MomentDataType="MOMENT_DATA_IMPROVEMENT"
			 GameDataType="IMPROVEMENT_FAIRY_FOUNTAIN"
			 Texture="YNL_MOMENT_FAIRY_FOUNTAIN.dds"/>
		<!-- 野戦救急車 -->
		<Row MomentIllustrationType="MOMENT_ILLUSTRATION_UNIQUE_UNIT"
			 MomentDataType="MOMENT_DATA_UNIT"
			 GameDataType="UNIT_HYLIAN_AMBULANCE"
			 Texture="YNL_MOMENT_FIELD_AMBULANCE.dds"/>
	</MomentIllustrations>
</GameData>
